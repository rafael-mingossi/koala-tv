{
  "expo": {
    "name": "Koala TV",
    "slug": "koala-tv",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/koala_icon.png",
    "splash": {
      "image": "./assets/koala_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.koalatv.koalatv",
      "supportsTablet": true
    },
    "android": {
      "package": "com.koalatv.koalatv",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
